programa
{
	
	funcao inicio()
	{
   real temperatura
  
   escreva("Digite a temperatura: ")
   leia(temperatura)
   

   se (temperatura > 0)
      escreva("A temperatura é positiva.")
   
   senao
      se (temperatura < 0)
         escreva("A temperatura é negativa.")
         
      senao
         escreva("A temperatura é 0.")

	}
}
