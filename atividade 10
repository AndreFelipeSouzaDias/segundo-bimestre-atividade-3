programa
{
	
	funcao inicio()
 {
   real velocidade, excesso, multa
   
   escreva("Digite a velocidade do carro (em km/h): ")
   leia(velocidade)

   se (velocidade > 80) {
      excesso = velocidade - 80
      multa = excesso * 7
      escreva("VocÃª foi multado!")
      escreva("Valor da multa: R$", multa)
   } senao {
      escreva("Velocidade dentro do limite. Sem multa.")
   }
 }
}
